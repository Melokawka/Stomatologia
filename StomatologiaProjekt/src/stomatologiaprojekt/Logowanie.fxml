<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stomatologiaprojekt.LogowanieController">
    <children>
        <Button fx:id="button" layoutX="133.0" layoutY="137.0" onAction="#handleButtonAction" text="Zaloguj" />
        <Label fx:id="label" layoutX="39.0" layoutY="48.0" minHeight="16" minWidth="40.0" prefHeight="25.0" prefWidth="40.0" text="login:">
         <font>
            <Font size="14.0" />
         </font></Label>
      <TextField fx:id="login" layoutX="86.0" layoutY="48.0" onKeyPressed="#handleEnter" />
      <Label fx:id="label1" layoutX="39.0" layoutY="88.0" minHeight="16" minWidth="40.0" prefHeight="25.0" prefWidth="40.0" text="hasło:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <PasswordField fx:id="haslo" layoutX="86.0" layoutY="88.0" onKeyPressed="#handleEnter" />
      <Label fx:id="incorrect" layoutX="87.0" layoutY="14.0" prefHeight="18.0" prefWidth="177.0" text="Błędne login lub hasło!" textFill="RED" />
    </children>
</AnchorPane>
